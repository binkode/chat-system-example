{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-pcntl": "*",
    "ext-redis": "*",
    "ext-swoole": "*",
    "fakerphp/faker": "^1.19",
    "fruitcake/laravel-cors": "^3.0",
    "guzzlehttp/guzzle": "^7.4",
    "inertiajs/inertia-laravel": "^0.5.4",
    "innocenzi/laravel-vite": "^0.1.17",
    "laravel/framework": "^9.9",
    "laravel/octane": "^1.2",
    "laravel/sanctum": "^2.15",
    "laravel/telescope": "^4.8",
    "laravel/tinker": "^2.7",
    "myckhel/laravel-chat-system": "dev-master",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "^7.0",
    "tightenco/ziggy": "^1.4"
  },
  "require-dev": {
    "spatie/laravel-ignition": "^1.0",
    "jeroen-g/laravel-packager": "^2.5",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.3.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "php artisan octane:start --workers=1 --task-workers=1 --watch"
    ]
  },
  "repositories": {
    "myckhel/laravel-chat-system": {
      "type": "vcs",
      "url": "https://github.com/myckhel/laravel-chat-system.git",
      "options": {
        "symlink": true
      }
    }
  }
}
